<aura:component implements="flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId" access="global" >
<aura:attribute name="myBreadcrumbs" type="Object"/>    
<aura:handler name="init" value="{!this}" action="{!c.init}" />
<lightning:avatar src="/resource/Avtar" size="x-large" variant="square" initials="BW" fallbackIconName="standard_person_account" alternativeText="Bob Wilson" />
  <div> 
    <lightning:badge label="label" />
</div>
<lightning:breadcrumbs>
    <lightning:breadcrumb label="Parent Account" href="url" onclick="{!c.naviageToParentAccount}"/>
    <lightning:breadcrumb label="case" href="url" onclick="{!c.navigateToCase}"/>
</lightning:breadcrumbs>
<lightning:breadcrumbs>
    <aura:iteration items="{!v.myBreadcrumbs}" var="crumbs">
        <lightning:breadcrumb label="{!crumbs.label}" onclick="{!c.naviagateTo}" />
    </aura:iteration>
    
</lightning:breadcrumbs>
<lightning:buttonGroup>
    
 al" label="Neutral" title="Neutral action" onclick="{! c.handleClick }"/>
<!--Brand variant-->
<lightning:button variant="brand" label="Brand" title="Brand Action" onclick="{! c.handleClick }"/>
<lightning:buttonIcon iconName="iconName" variant="border" size="xx-small"  alternativeText="alternativeText" />
<lightning:buttonMenu alternativeText="Show menu" class="">
    <lightning:menuItem value="Item1" label="Menu Item One" />
    <lightning:menuItem value="value" label="Menu item Two" />
</lightning:buttonMenu>
</lightning:buttonGroup>
</aura:component>